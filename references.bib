issn = {01421123},
journal = {International Journal of Fatigue},
keywords = {Crack detection,Damping,Fatigue,Hastelloy-X,Nondestructive testing},
pages = {300--309},
title = {Resonance bending fatigue testing with simultaneous damping measurement and its application on layered coatings},
