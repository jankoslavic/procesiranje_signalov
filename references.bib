keywords = {Corrosion,Dynamic loads,Fatigue damage,Hurricane,Long-span bridges,Non-stationary process,Vehicle-bridge-wind dynamic system,Wind-induced vibrations},
pages = {135--145},
title = {{Fatigue life estimation of existing bridges under vehicle and non-stationary hurricane wind}},
volume = {133},
year = {2014}
}
